{
  "name": "litphp/nimo",
  "description": "Nimo Is your Middleware Organizer",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "middleware",
    "psr",
    "psr-7",
    "psr-15",
    "http",
    "http-middleware"
  ],
  "authors": [
    {
      "name": "McFog Wang",
      "email": "mcfogw@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Lit\\Nimo\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "require": {
    "php": ">=7.1.0",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0"
  },
  "minimum-stability": "dev",
  "scripts": {
    "test": "phpunit -c ."
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "require-dev": {
    "laminas/laminas-diactoros": "^2.3",
    "phpunit/phpunit": "^7.5"
  }
}
